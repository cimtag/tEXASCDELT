<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="1.1" STATUS="PROD"
		COMPATIBILITY="ALL" AUTHOR="cimt AG" RELEASE_DATE="20161212A"
		STARTABLE="true" SCHEMA_AUTO_PROPAGATE="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Databases/EXASolution</FAMILY>
		<FAMILY>Business_Intelligence/DB_SCD</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="0" MAX_INPUT="0"
			MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" GROUP="CONNECTION_CONFIG"
			NUM_ROW="10" REPOSITORY_VALUE="DATABASE:EXASOL" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" />

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="11" GROUP="CONNECTION_CONFIG">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" FILTER="tEXAConnection" NUM_ROW="11" SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
			DYNAMIC_SETTINGS="true" GROUP="CONNECTION_CONFIG" />

		<PARAMETER NAME="HOST" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="30" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="SERVER_NAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PORT" GROUP="CONNECTION_CONFIG" FIELD="TEXT"
			NUM_ROW="30" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="PORT">
			<DEFAULT>"8563"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION_PROPERTIES" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="31" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" REQUIRED="false">
			<DEFAULT>"retrieveMessagesFromServerOnGetMessage=true"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="32" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="SID" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_SCHEMA" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="32" REPOSITORY_VALUE="SCHEMA" REQUIRED="true"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="40" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="USERNAME" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="TEXT" GROUP="CONNECTION_CONFIG"
			NUM_ROW="40" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="PASSWORD" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>


		<!-- SOURCE -->
		<PARAMETER NAME="TABLE" FIELD="DBTABLE" GROUP="SOURCE_CONFIG"
			NUM_ROW="50" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="WHERE" FIELD="MEMO" GROUP="SOURCE_CONFIG"
			NUM_ROW="51" NB_LINES="3">
			<DEFAULT/>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" GROUP="SOURCE_CONFIG"
			NUM_ROW="52">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ENABLE_IGNORE" FIELD="CHECK" GROUP="SOURCE_CONFIG"
			NUM_ROW="53">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ENABLE_SCD1" FIELD="CHECK" GROUP="SOURCE_CONFIG"
			NUM_ROW="53">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ENABLE_SCD2" FIELD="CHECK" GROUP="SOURCE_CONFIG"
			NUM_ROW="53">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ENABLE_SCD3" FIELD="CHECK" GROUP="SOURCE_CONFIG"
			NUM_ROW="53">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_SELF_DEFINED_SOURCE_KEY" GROUP="SOURCE_CONFIG" FIELD="CHECK"
			NUM_ROW="54" DYNAMIC_SETTINGS="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ENABLE_VALID_FROM_SOURCE_COLUMN" GROUP="SOURCE_CONFIG" FIELD="CHECK"
			NUM_ROW="54" DYNAMIC_SETTINGS="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SUPPLEMENTARY_SOURCE_COLUMN_CONFIG" FIELD="TABLE"
			NUM_ROW="55" NB_LINES="10" GROUP="SOURCE_CONFIG" DYNAMIC_SETTINGS="true">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="IGNORE" FIELD="CHECK" VALUE="false" SHOW_IF="ENABLE_IGNORE == 'true'" />
				<ITEM NAME="IS_SOURCE_KEY" FIELD="CHECK" VALUE="false" SHOW_IF="USE_SELF_DEFINED_SOURCE_KEY == 'true'" />
				<ITEM NAME="TIME_STAMP" FIELD="CHECK" VALUE="false" SHOW_IF="ENABLE_VALID_FROM_SOURCE_COLUMN == 'true'" />
				<ITEM NAME="SCD1A" FIELD="CHECK" VALUE="false" SHOW_IF="ENABLE_SCD1 == 'true'" />
				<ITEM NAME="SCD1B" FIELD="CHECK" VALUE="false" SHOW_IF="(ENABLE_SCD1 == 'true' and ENABLE_SCD2 == 'true')" />
				<ITEM NAME="SCD2" FIELD="CHECK" VALUE="true" SHOW_IF="ENABLE_SCD2 == 'true'" />
				<ITEM NAME="SCD3" FIELD="CHECK" VALUE="false" SHOW_IF="ENABLE_SCD3 == 'true'" />
				<ITEM NAME="SCD3_ADDITIONAL_TARGET_COLUMN" FIELD="TEXT" SHOW_IF="ENABLE_SCD3 == 'true'" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="VALID_FROM_VALUE" FIELD="TEXT" GROUP="SOURCE_CONFIG"
			NUM_ROW="57">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>


		<!-- TARGET -->
		<PARAMETER NAME="TARGET_SCHEMA" GROUP="TARGET_CONFIG" FIELD="TEXT"
			NUM_ROW="60" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_TABLE" GROUP="TARGET_CONFIG" FIELD="TEXT"
			NUM_ROW="60" REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CREATE_TABLE_IF_NOT_EXISTS" FIELD="CHECK" GROUP="TARGET_CONFIG" NUM_ROW="62" />

		<PARAMETER NAME="CREATE_PK" FIELD="CHECK" GROUP="TARGET_CONFIG" NUM_ROW="62" SHOW_IF="CREATE_TABLE_IF_NOT_EXISTS == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CREATE_SK" FIELD="CHECK" GROUP="TARGET_CONFIG" NUM_ROW="62" SHOW_IF="CREATE_TABLE_IF_NOT_EXISTS == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SURROGATE_KEY" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="62" SHOW_IF="(CREATE_SK == 'true' and CREATE_TABLE_IF_NOT_EXISTS == 'true')">
			<DEFAULT>"SKEY"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_CURRENT_FLAG" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="65" REQUIRED="true" SHOW_IF="ENABLE_SCD2 == 'true'" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_CURRENT_FLAG_COLUMN" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="65" SHOW_IF="(ENABLE_SCD2 == 'true' and USE_CURRENT_FLAG == 'true')">
			<DEFAULT>"CUR_FLAG"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_VERSION" FIELD="CHECK" GROUP="TARGET_CONFIG"
			NUM_ROW="66" REQUIRED="true" SHOW_IF="ENABLE_SCD2 == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_VERSION_COLUMN" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="66" SHOW_IF="(ENABLE_SCD2 == 'true' and USE_VERSION == 'true')">
			<DEFAULT>"VERSION"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_VERSION_START" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="66" SHOW_IF="(ENABLE_SCD2 == 'true' and USE_VERSION == 'true')">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="VALID_TIMEPERIOD_START" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="67" REQUIRED="true" SHOW_IF="ENABLE_SCD2 == 'true'" >
			<DEFAULT>"VALID_FROM"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="VALID_TIMEPERIOD_END" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="67" REQUIRED="true" SHOW_IF="ENABLE_SCD2 == 'true'" >
			<DEFAULT>"VALID_TO"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="TIME_OF_LAST_CHANGE" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="67" REQUIRED="true" SHOW_IF="(ENABLE_SCD2 == 'false' and ENABLE_SCD3 == 'true')" >
			<DEFAULT>"TIME_OF_LAST_CHANGE"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="VALID_TIMEPERIOD_MAX_VALUE" FIELD="TEXT" GROUP="TARGET_CONFIG"
			NUM_ROW="68" REQUIRED="true" SHOW_IF="ENABLE_SCD2 == 'true'" >
			<DEFAULT>"9999-01-01"</DEFAULT>
		</PARAMETER>


		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="75">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>

		<PARAMETER NAME="NO_EXEC_MODE" FIELD="CHECK" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DEBUG" FIELD="CHECK" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LOG_STATS" FIELD="CHECK" NUM_ROW="30">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LOG_STATS_FILE" FIELD="FILE" NUM_ROW="40">
			<DEFAULT/>
		</PARAMETER>

		<PARAMETER 
			NAME="MAPPING" 
			FIELD="MAPPING_TYPE" 
			NUM_ROW="910"
			SHOW="false">
			<DEFAULT>Exasol_id</DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="cimt-talendcomp-exascdelt" MODULE="cimt-talendcomp-exascdelt-0.1.0-SNAPSHOT.jar"
				REQUIRED="true" />
			<IMPORT NAME="Driver-EXASolution" MODULE="exajdbc-5.0.16.jar" MVN="mvn:org.talend.libraries/exajdbc-5.0.16/6.3.0"  REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="COUNT_SCD2_INSERT_NEW_RECORDS" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="COUNT_SCD1_CHANGED_RECORDS" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="COUNT_SCD2_CHANGED_RECORDS" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="COUNT_SCD3_CHANGED_RECORDS" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="COUNT_OUTDATED_RECORDS" TYPE="id_Integer" />
	</RETURNS>

</COMPONENT>